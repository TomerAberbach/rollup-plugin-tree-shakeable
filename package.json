{
  "name": "rollup-plugin-tree-shakeable",
  "version": "2.0.0",
  "author": {
    "name": "Tomer Aberbach",
    "email": "tomer@aberba.ch",
    "url": "https://tomeraberba.ch"
  },
  "description": "A Rollup plugin that automatically annotates your module as tree shakeable.",
  "keywords": [
    "bundle",
    "pure",
    "rollup",
    "rollup-plugin",
    "tree-shaking"
  ],
  "homepage": "https://github.com/TomerAberbach/rollup-plugin-tree-shakeable",
  "repository": "TomerAberbach/rollup-plugin-tree-shakeable",
  "bugs": {
    "url": "https://github.com/TomerAberbach/rollup-plugin-tree-shakeable/issues"
  },
  "funding": {
    "url": "https://github.com/sponsors/TomerAberbach"
  },
  "license": "Apache-2.0 AND MIT",
  "files": [
    "dist",
    "license-apache",
    "license-mit",
    "notice-apache"
  ],
  "type": "module",
  "sideEffects": false,
  "engines": {
    "node": ">= 22"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "format": "prettier --cache --write .",
    "lint": "eslint --cache --cache-location node_modules/.cache/eslint/ --fix .",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "coverage": "vitest --coverage",
    "bench": "vitest bench",
    "build": "tsdown",
    "prepublishOnly": "pnpm build"
  },
  "prettier": "@tomer/prettier-config",
  "dependencies": {
    "@types/node": "^24.3.0",
    "estree-walker": "^3.0.3",
    "magic-string": "^0.30.18"
  },
  "devDependencies": {
    "@tomer/eslint-config": "^4.1.1",
    "@tomer/prettier-config": "^4.0.0",
    "@types/estree": "^1.0.8",
    "@vitest/coverage-v8": "^3.2.4",
    "esbuild": "^0.25.9",
    "eslint": "^9.34.0",
    "prettier": "^3.6.2",
    "publint": "^0.3.12",
    "rollup": "^4.50.0",
    "tsdown": "^0.14.2",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  }
}
